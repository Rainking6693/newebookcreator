# Network timeouts and retries
fetch-timeout=300000
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000

# Registry settings
registry=https://registry.npmjs.org/
always-auth=false

# Disable audit and funding to reduce network calls
audit=false
fund=false

# Prefer offline cache
prefer-offline=true

# Disable progress to reduce log noise
progress=false