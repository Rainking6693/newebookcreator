# API Configuration (update with your actual backend URL)
REACT_APP_API_URL=https://your-backend-domain.com
REACT_APP_API_VERSION=v1

# Stripe Configuration (use your actual Stripe publishable key)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Analytics (optional)
REACT_APP_GA_TRACKING_ID=GA-XXXX-X
REACT_APP_HOTJAR_ID=your_hotjar_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT=false

# Environment
REACT_APP_ENVIRONMENT=production

# Site Configuration
REACT_APP_SITE_NAME=StartupNamer.ai
REACT_APP_SITE_URL=https://startupnamer.ai